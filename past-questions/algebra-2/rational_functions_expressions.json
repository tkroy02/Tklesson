      "step_by_step": [
        {
          "step": "Set denominator not equal to zero",
          "calculation": "$x - 5 \\neq 0 \\Rightarrow x \\neq 5$"
        }
      ],
      "final_answer": "Domain: $x \\in \\mathbb{R}, x \\neq 5$"
    },
    {
      "number": 8,
      "problem": "Simplify: $\\frac{x^2 + 5x + 6}{x + 2}$",
      "image": null,
      "step_by_step": [
        {
          "step": "Factor numerator",
          "calculation": "$x^2 + 5x + 6 = (x + 2)(x + 3)$"
        },
        {
          "step": "Cancel common factor",
          "calculation": "$(x + 2)(x + 3)/(x + 2) = x + 3$"
        }
      ],
      "final_answer": "$x + 3$"
    },
    {
