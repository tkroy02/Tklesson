  "problems": [
    {
      "number": 1,
      "problem": "Simplify: $\\frac{2x}{4x}$",
      "image": null,
      "step_by_step": [
        {
          "step": "Cancel common factor of $2x$",
          "calculation": "$\\frac{2x}{4x} = \\frac{1}{2}$"
        }
      ],
      "final_answer": "$\\frac{1}{2}$"
    },
    {
      "number": 2,
      "problem": "Simplify: $\\frac{x^2 - 9}{x - 3}$",
      "image": null,
      "step_by_step": [
        {
          "step": "Factor numerator as difference of squares",
          "calculation": "$x^2 - 9 = (x - 3)(x + 3)$"
        },
        {
          "step": "Cancel common factor",
          "calculation": "$(x - 3)(x + 3)/(x - 3) = x + 3$"
        }
      ],
      "final_answer": "$x + 3$"
    },
    {
      "number": 3,
      "problem": "Simplify: $\\frac{x^2 - 4x + 4}{x - 2}$",
      "image": null,
      "step_by_step": [
        {
          "step": "Factor numerator as perfect square",
          "calculation": "$x^2 - 4x + 4 = (x - 2)^2$"
        },
        {
          "step": "Cancel common factor",
          "calculation": "$(x - 2)^2 / (x - 2) = x - 2$"
        }
      ],
      "final_answer": "$x - 2$"
    },
    {
      "number": 4,
      "problem": "Multiply: $\\frac{2x}{3} \\cdot \\frac{9}{4x}$",
      "image": null,
      "step_by_step": [
        {
          "step": "Multiply numerators and denominators",
          "calculation": "$\\frac{2x \\cdot 9}{3 \\cdot 4x} = \\frac{18x}{12x}$"
        },
        {
          "step": "Simplify fraction",
          "calculation": "$\\frac{18x}{12x} = \\frac{3}{2}$"
        }
      ],
      "final_answer": "$\\frac{3}{2}$"
    },
    {
      "number": 5,
      "problem": "Divide: $\\frac{x^2 - 1}{x + 1} \\div \\frac{x - 1}{2}$",
      "image": null,
      "step_by_step": [
        {
          "step": "Factor numerator",
          "calculation": "$x^2 - 1 = (x - 1)(x + 1)$"
        },
        {
          "step": "Rewrite division as multiplication",
          "calculation": "$\\frac{(x - 1)(x + 1)}{x + 1} \\cdot \\frac{2}{x - 1}$"
        },
        {
          "step": "Cancel common factors",
          "calculation": "$2$"
        }
      ],
      "final_answer": "$2$"
    },
    {
      "number": 6,
      "problem": "Simplify: $\\frac{3x^2y}{6xy^2}$",
      "image": null,
      "step_by_step": [
        {
          "step": "Simplify coefficients and variables",
          "calculation": "$\\frac{3}{6} \\cdot \\frac{x^2}{x} \\cdot \\frac{y}{y^2} = \\frac{1}{2} \\cdot x \\cdot \\frac{1}{y} = \\frac{x}{2y}$"
        }
      ],
      "final_answer": "$\\frac{x}{2y}$"
    },
    {
      "number": 7,
