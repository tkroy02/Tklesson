{
  "course": "AP Physics 2",
  "topic": "Fluids",
  "problems": [
    {
      "number": 1,
      "problem": "A block of volume 0.02 m³ floats in oil of density $800 \\, \\text{kg/m³}$. Find the buoyant force.",
      "image": null,
      "step_by_step": [
        {
          "step": "Buoyant force formula",
          "explanation": "$F_B = \\rho g V$"
        },
        {
          "step": "Substitute values",
          "calculation": "$F_B = 800 * 9.8 * 0.02 = 156.8 \\, \\text{N}$"
        }
      ],
      "final_answer": "Buoyant force: $F_B = 156.8 \\, \\text{N}$"
    },
    {
      "number": 2,
      "problem": "A pressure gauge reads 2.5 × 10⁴ Pa on a surface area of 0.1 m². Find the force.",
      "image": null,
      "step_by_step": [
        {
          "step": "Pressure-force relationship",
          "explanation": "$F = P A$"
        },
        {
          "step": "Substitute values",
          "calculation": "$F = 2.5 \\times 10^4 * 0.1 = 2500 \\, \\text{N}$"
        }
      ],
      "final_answer": "Force on surface: $F = 2500 \\, \\text{N}$"
    },
    {
      "number": 3,
      "problem": "Water has a density of $1000 \\, \\text{kg/m³}$. Find the pressure at depth 10 m.",
      "image": null,
      "step_by_step": [
        {
          "step": "Hydrostatic pressure",
          "explanation": "$P = \\rho g h$"
        },
        {
          "step": "Substitute values",
          "calculation": "$P = 1000 * 9.8 * 10 = 98000 \\, \\text{Pa}$"
        }
      ],
      "final_answer": "Pressure at 10 m: $P = 98000 \\, \\text{Pa}$"
    },
    {
      "number": 4,
      "problem": "A 5 kg object floats in water. What fraction of its volume is submerged?",
      "image": null,
      "step_by_step": [
        {
          "step": "Use buoyant force = weight",
          "explanation": "$F_B = mg = \\rho g V_{sub}$"
        },
        {
          "step": "Solve fraction submerged",
          "calculation": "$V_{sub}/V = 5*9.8 / (1000*9.8) = 0.005$"
        }
      ],
      "final_answer": "Fraction submerged: $0.005$ (0.5%)"
    },
    {
      "number": 5,
      "problem": "Water flows through a 0.03 m² pipe at 4 m/s. Find the volume flow rate.",
      "image": null,
      "step_by_step": [
        {
          "step": "Volume flow rate",
          "explanation": "$Q = A v$"
        },
        {
          "step": "Substitute values",
          "calculation": "$Q = 0.03 * 4 = 0.12 \\, \\text{m³/s}$"
        }
      ],
      "final_answer": "Volume flow rate: $Q = 0.12 \\, \\text{m³/s}$"
    },
    {
      "number": 6,
      "problem": "A hydraulic lift has piston areas $A_1 = 0.01 \\, \\text{m²}$ and $A_2 = 0.4 \\, \\text{m²}$. If $F_1 = 100 N$, find $F_2$.",
      "image": null,
      "step_by_step": [
        {
          "step": "Pascal's principle",
          "explanation": "$F_1/A_1 = F_2/A_2 \\Rightarrow F_2 = F_1 A_2 / A_1$"
        },
        {
          "step": "Substitute values",
          "calculation": "$F_2 = 100 * 0.4 / 0.01 = 4000 \\, N$"
        }
      ],
      "final_answer": "Output force: $F_2 = 4000 \\, N$"
    },
    {
      "number": 7,
      "problem": "A pipe narrows from 0.12 m² to 0.04 m². Velocity in wider section is 2 m/s. Find velocity in narrow section.",
      "image": null,
      "step_by_step": [
        {
          "step": "Continuity equation",
          "explanation": "$A_1 v_1 = A_2 v_2 \\Rightarrow v_2 = A_1 v_1 / A_2$"
        },
        {
          "step": "Substitute values",
          "calculation": "$v_2 = 0.12 * 2 / 0.04 = 6 \\, \\text{m/s}$"
        }
      ],
      "final_answer": "Velocity in narrow section: $v_2 = 6 \\, \\text{m/s}$"
    },
    {
      "number": 8,
      "problem": "Water exits a tank from a hole at depth 6 m. Find exit speed.",
      "image": null,
      "step_by_step": [
        {
          "step": "Torricelli's theorem",
          "explanation": "$v = \\sqrt{2 g h}$"
        },
        {
          "step": "Substitute values",
          "calculation": "$v = \\sqrt{2 * 9.8 * 6} = \\sqrt{117.6} \\approx 10.84 \\, \\text{m/s}$"
        }
      ],
      "final_answer": "Exit speed: $v \\approx 10.84 \\, \\text{m/s}$"
    },
    {
      "number": 9,
      "problem": "A cube of side 0.05 m is fully submerged in water. Find buoyant force.",
      "image": null,
      "step_by_step": [
        {
          "step": "Buoyant force",
          "explanation": "$F_B = \\rho g V = 1000 * 9.8 * 0.05^3$"
        },
        {
          "step": "Calculate",
          "calculation": "$F_B = 1000 * 9.8 * 0.000125 = 1.225 \\, \\text{N}$"
        }
      ],
      "final_answer": "Buoyant force: $F_B = 1.225 \\, \\text{N}$"
    },
    {
      "number": 10,
      "problem": "Air flows through a 0.015 m² pipe at 6 m/s. Find mass flow rate. Air density $1.2 \\, \\text{kg/m³}$.",
      "image": null,
      "step_by_step": [
        {
          "step": "Mass flow rate",
          "explanation": "$\\dot{m} = \\rho Q = \\rho A v$"
        },
        {
          "step": "Substitute values",
          "calculation": "$\\dot{m} = 1.2 * 0.015 * 6 = 0.108 \\, \\text{kg/s}$"
        }
      ],
      "final_answer": "Mass flow rate: $\\dot{m} = 0.108 \\, \\text{kg/s}$"
    }
  ]
