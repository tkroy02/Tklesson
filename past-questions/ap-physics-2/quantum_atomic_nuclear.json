      "number": 1,
      "problem": "Calculate the energy of a photon with wavelength $500 \\, \\text{nm}$.",
      "image": null,
      "step_by_step": [
        {
          "step": "Use photon energy formula",
          "explanation": "$E = \\frac{hc}{\\lambda}$"
        },
        {
          "step": "Substitute values",
          "explanation": "$h = 6.63 \\times 10^{-34} \\, \\text{Js}, c = 3 \\times 10^8 \\, \\text{m/s}, \\lambda = 500 \\times 10^{-9} \\, \\text{m}$",
          "calculation": "$E = \\frac{6.63\\times10^{-34} * 3\\times10^8}{500\\times10^{-9}} \\approx 3.978 \\times 10^{-19} \\, \\text{J}$"
        }
      ],
      "final_answer": "Photon energy: $E \\approx 3.98 \\times 10^{-19} \\, \\text{J}$"
