  "problems": [
    {
      "number": 1,
      "problem": "A 12 V battery is connected to a resistor of 4 Ω. Find the current.",
      "image": null,
      "step_by_step": [
        {
          "step": "Ohm's Law",
          "explanation": "$I = V / R$"
        },
        {
          "step": "Substitute values",
          "calculation": "$I = 12 / 4 = 3 \\, \\text{A}$"
        }
      ],
      "final_answer": "Current: $I = 3 \\, \\text{A}$"
    },
    {
      "number": 2,
      "problem": "A 9 V battery is connected to two resistors in series, 6 Ω and 3 Ω. Find total current.",
      "image": null,
      "step_by_step": [
        {
          "step": "Total resistance in series",
          "explanation": "$R_{total} = R_1 + R_2 = 6 + 3 = 9 \\, \\Omega$"
        },
        {
          "step": "Ohm's Law",
          "explanation": "$I = V / R_{total}$",
          "calculation": "$I = 9 / 9 = 1 \\, \\text{A}$"
        }
      ],
      "final_answer": "Current: $I = 1 \\, \\text{A}$"
    },
    {
      "number": 3,
      "problem": "Two resistors, 8 Ω and 12 Ω, are in parallel across 24 V. Find total current drawn.",
      "image": null,
      "step_by_step": [
        {
          "step": "Total resistance in parallel",
          "explanation": "$1/R_{total} = 1/8 + 1/12$"
        },
        {
          "step": "Calculate $R_{total}$",
          "calculation": "$1/R_{total} = 3/24 + 2/24 = 5/24 \\implies R_{total} = 24/5 = 4.8 \\, \\Omega$"
        },
        {
          "step": "Ohm's Law",
          "calculation": "$I = V / R_{total} = 24 / 4.8 = 5 \\, \\text{A}$"
        }
      ],
      "final_answer": "Total current: $I = 5 \\, \\text{A}$"
    },
    {
      "number": 4,
      "problem": "A circuit has a 6 Ω resistor and a 12 Ω resistor in series with 18 V. Find voltage across each resistor.",
      "image": null,
      "step_by_step": [
        {
          "step": "Total resistance",
          "explanation": "$R_{total} = 6 + 12 = 18 \\, \\Omega$"
        },
        {
          "step": "Current in series",
          "calculation": "$I = V / R_{total} = 18 / 18 = 1 \\, \\text{A}$"
        },
        {
          "step": "Voltage across resistors",
          "calculation": "V_1 = I R_1 = 1*6 = 6\\,V, \\quad V_2 = I R_2 = 1*12 = 12\\,V"
        }
      ],
      "final_answer": "Voltage across resistors: $V_1 = 6 \\, \\text{V}, V_2 = 12 \\, \\text{V}$"
    },
    {
      "number": 5,
      "problem": "A 20 Ω resistor carries 2 A current. Find power dissipated.",
      "image": null,
      "step_by_step": [
        {
          "step": "Power formula",
          "explanation": "$P = I^2 R$"
        },
        {
          "step": "Substitute values",
          "calculation": "$P = 2^2 * 20 = 4 * 20 = 80 \\, \\text{W}$"
        }
      ],
      "final_answer": "Power: $P = 80 \\, \\text{W}$"
    },
    {
      "number": 6,
      "problem": "A 24 V battery is connected to a 6 Ω resistor. How much energy is dissipated in 5 seconds?",
      "image": null,
      "step_by_step": [
        {
          "step": "Current",
          "explanation": "$I = V / R = 24 / 6 = 4 \\, \\text{A}$"
        },
        {
          "step": "Power",
          "explanation": "$P = I^2 R = 4^2 * 6 = 16 * 6 = 96 \\, \\text{W}$"
        },
        {
          "step": "Energy",
          "explanation": "$E = P t = 96 * 5 = 480 \\, \\text{J}$"
        }
      ],
      "final_answer": "Energy dissipated: $E = 480 \\, \\text{J}$"
    },
    {
      "number": 7,
      "problem": "A parallel circuit has 3 Ω and 6 Ω resistors connected to 12 V. Find individual branch currents.",
