        {
          "step": "Median: order and pick middle",
          "calculation": "4, 6, 8, 8, 10 \\Rightarrow \\text{Median} = 8$"
        },
        {
          "step": "Mode: most frequent value",
          "calculation": "Mode = 8"
        }
      ],
      "final_answer": "Mean = $7.2$, Median = $8$, Mode = $8$"
