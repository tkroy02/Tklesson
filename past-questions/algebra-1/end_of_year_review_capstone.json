      "problem": "Solve for $x$: $2x + 5 = 13$",
      "image": null,
      "step_by_step": [
        {
          "step": "Subtract 5 from both sides",
          "calculation": "$2x = 13 - 5 = 8$"
        },
        {
          "step": "Divide both sides by 2",
          "calculation": "$x = 8 / 2 = 4$"
        }
      ],
      "final_answer": "$x = 4$"
    },
