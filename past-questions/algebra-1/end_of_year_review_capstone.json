      "problem": "Simplify $\\sqrt{50} + 3\\sqrt{2}$",
      "image": null,
      "step_by_step": [
        {
          "step": "Simplify $\\sqrt{50}$",
          "calculation": "$\\sqrt{50} = \\sqrt{25 \\cdot 2} = 5\\sqrt{2}$"
        },
        {
          "step": "Combine like radicals",
          "calculation": "$5\\sqrt{2} + 3\\sqrt{2} = 8\\sqrt{2}$"
        }
      ],
      "final_answer": "$8\\sqrt{2}$"
