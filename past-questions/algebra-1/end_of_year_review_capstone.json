    {
      "number": 4,
      "problem": "Solve the quadratic equation $x^2 - 5x + 6 = 0$",
      "image": null,
      "step_by_step": [
        {
          "step": "Factor quadratic",
          "calculation": "$(x - 2)(x - 3) = 0$"
        },
        {
          "step": "Set each factor equal to zero",
          "calculation": "$x - 2 = 0 \\Rightarrow x = 2$, $x - 3 = 0 \\Rightarrow x = 3$"
        }
      ],
      "final_answer": "$x = 2, 3$"
    },
    {
      "number": 5,
      "problem": "Find the slope and y-intercept of $y = 3x - 7$",
      "image": null,
      "step_by_step": [
        {
          "step": "Compare to slope-intercept form $y = mx + b$",
          "calculation": "Slope $m = 3$, y-intercept $b = -7$"
        }
      ],
      "final_answer": "Slope = $3$, y-intercept = $-7$"
    },
    {
      "number": 6,
      "problem": "Solve the system: $\\begin{cases} x + y = 7 \\\\ 2x - y = 3 \\end{cases}$",
      "image": null,
      "step_by_step": [
        {
          "step": "Add the two equations to eliminate $y$",
          "calculation": "$x + y + 2x - y = 7 + 3 \\Rightarrow 3x = 10 \\Rightarrow x = 10 / 3$"
        },
        {
          "step": "Substitute $x$ into first equation to find $y$",
          "calculation": "$10/3 + y = 7 \\Rightarrow y = 7 - 10/3 = 11/3$"
        }
      ],
      "final_answer": "$x = 10/3, y = 11/3$"
    },
