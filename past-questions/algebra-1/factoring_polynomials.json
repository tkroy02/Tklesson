  "topic": "Factoring Polynomials",
  "problems": [
    {
      "number": 1,
      "problem": "Factor completely: $x^2 + 5x + 6$",
      "image": null,
      "step_by_step": [
        {
          "step": "Find two numbers that multiply to 6 and add to 5",
          "calculation": "2 and 3"
        },
        {
          "step": "Write as factors",
          "calculation": "$(x + 2)(x + 3)$"
        }
      ],
      "final_answer": "$(x + 2)(x + 3)$"
    },
    {
      "number": 2,
      "problem": "Factor completely: $2x^2 + 7x + 3$",
      "image": null,
      "step_by_step": [
        {
          "step": "Multiply the coefficient of $x^2$ and the constant: $2 \\cdot 3 = 6$",
          "explanation": "Find two numbers that multiply to 6 and add to 7"
        },
        {
          "step": "Split the middle term",
          "calculation": "$2x^2 + 6x + x + 3$"
        },
        {
          "step": "Factor by grouping",
          "calculation": "$(2x^2 + 6x) + (x + 3) = 2x(x + 3) + 1(x + 3)$"
        },
        {
          "step": "Factor out the common binomial",
          "calculation": "$(2x + 1)(x + 3)$"
        }
      ],
      "final_answer": "$(2x + 1)(x + 3)$"
    },
    {
      "number": 3,
      "problem": "Factor completely: $x^2 - 9$",
      "image": null,
      "step_by_step": [
        {
          "step": "Recognize difference of squares",
          "explanation": "$a^2 - b^2 = (a - b)(a + b)$"
        },
        {
          "step": "Write as factors",
          "calculation": "$(x - 3)(x + 3)$"
        }
      ],
      "final_answer": "$(x - 3)(x + 3)$"
    },
    {
      "number": 4,
      "problem": "Factor completely: $x^2 + 6x + 9$",
      "image": null,
      "step_by_step": [
        {
          "step": "Recognize perfect square trinomial",
          "explanation": "$a^2 + 2ab + b^2 = (a + b)^2$"
        },
        {
          "step": "Write as square",
          "calculation": "$(x + 3)^2$"
        }
      ],
      "final_answer": "$(x + 3)^2$"
    },
    {
      "number": 5,
      "problem": "Factor completely: $x^3 + 8$",
      "image": null,
      "step_by_step": [
        {
          "step": "Recognize sum of cubes",
          "explanation": "$a^3 + b^3 = (a + b)(a^2 - ab + b^2)$"
        },
        {
          "step": "Write as sum of cubes",
          "calculation": "$x^3 + 8 = x^3 + 2^3$"
        },
        {
          "step": "Factor using formula",
          "calculation": "$(x + 2)(x^2 - 2x + 4)$"
        }
      ],
      "final_answer": "$(x + 2)(x^2 - 2x + 4)$"
    },
    {
      "number": 6,
      "problem": "Factor completely: $x^3 - 27$",
      "image": null,
      "step_by_step": [
        {
          "step": "Recognize difference of cubes",
          "explanation": "$a^3 - b^3 = (a - b)(a^2 + ab + b^2)$"
        },
        {
          "step": "Write as difference of cubes",
          "calculation": "$x^3 - 27 = x^3 - 3^3$"
        },
        {
          "step": "Factor using formula",
          "calculation": "$(x - 3)(x^2 + 3x + 9)$"
        }
      ],
      "final_answer": "$(x - 3)(x^2 + 3x + 9)$"
    },
    {
      "number": 7,
      "problem": "Factor completely: $x^2 + 7x + 10$",
      "image": null,
      "step_by_step": [
        {
          "step": "Find two numbers that multiply to 10 and add to 7",
          "calculation": "2 and 5"
        },
        {
          "step": "Write as factors",
          "calculation": "$(x + 2)(x + 5)$"
        }
      ],
      "final_answer": "$(x + 2)(x + 5)$"
    },
    {
      "number": 8,
      "problem": "Factor completely: $3x^2 - 12$",
      "image": null,
      "step_by_step": [
        {
          "step": "Factor out the greatest common factor",
          "calculation": "$3(x^2 - 4)$"
        },
        {
          "step": "Factor difference of squares",
          "calculation": "$3(x - 2)(x + 2)$"
        }
      ],
      "final_answer": "$3(x - 2)(x + 2)$"
    },
    {
      "number": 9,
      "problem": "Factor completely: $2x^3 + 6x^2$",
      "image": null,
      "step_by_step": [
        {
          "step": "Factor out the greatest common factor",
          "calculation": "$2x^2(x + 3)$"
        }
      ],
      "final_answer": "$2x^2(x + 3)$"
    },
    {
      "number": 10,
      "problem": "Factor completely: $x^4 - 16$",
      "image": null,
      "step_by_step": [
        {
          "step": "Recognize difference of squares",
          "calculation": "$x^4 - 16 = (x^2)^2 - 4^2 = (x^2 - 4)(x^2 + 4)$"
        },
        {
          "step": "Factor further if possible",
          "calculation": "$(x - 2)(x + 2)(x^2 + 4)$"
        }
      ],
      "final_answer": "$(x - 2)(x + 2)(x^2 + 4)$"
    }
  ]
