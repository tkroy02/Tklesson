{
  "course": "Physics Grade 11",
  "topic": "Fluid Mechanics",
  "problems": [
    {
      "number": 1,
      "problem": "A cube of side $0.1\\,\\text{m}$ is submerged in water. Find the buoyant force acting on it. Density of water $\\rho = 1000\\,\\text{kg/m}^3$, $g = 9.8\\,\\text{m/s}^2$.",
      "image": null,
      "step_by_step": [
        {
          "step": "Identify volume",
          "explanation": "Volume of cube $V = (0.1)^3 = 0.001\\,\\text{m}^3$"
        },
        {
          "step": "Apply Archimedes’ principle",
          "explanation": "Buoyant force $F_b = \\rho g V$",
          "calculation": "$$ F_b = 1000 \\times 9.8 \\times 0.001 = 9.8\\,\\text{N} $$"
        }
      ],
      "final_answer": "The buoyant force is $9.8\\,\\text{N}$."
    },
    {
      "number": 2,
      "problem": "A block of wood floats on water with 1/4 of its volume above water. Find the density of the wood. Density of water $\\rho_w = 1000\\,\\text{kg/m}^3$.",
      "image": null,
      "step_by_step": [
        {
          "step": "Relation",
          "explanation": "For floating: $\\rho_{wood} / \\rho_w = V_{sub}/V_{total}$"
        },
        {
          "step": "Substitute",
          "calculation": "$$ \\rho_{wood}/1000 = 3/4 \\implies \\rho_{wood} = 750\\,\\text{kg/m}^3 $$"
        }
      ],
      "final_answer": "Density of the wood is $750\\,\\text{kg/m}^3$."
    },
    {
      "number": 3,
      "problem": "A liquid has a pressure of $2\\,\\text{Pa}$ at a depth of $0.2\\,\\text{m}$. Find the density of the liquid. $g = 10\\,\\text{m/s}^2$.",
      "image": null,
      "step_by_step": [
        {
          "step": "Hydrostatic pressure",
          "explanation": "$P = \\rho g h$"
        },
        {
          "step": "Substitute",
          "calculation": "$$ 2 = \\rho \\times 10 \\times 0.2 \\implies \\rho = 1\\,\\text{kg/m}^3 $$"
        }
      ],
      "final_answer": "Density of the liquid is $1\\,\\text{kg/m}^3$."
    },
    {
      "number": 4,
      "problem": "A U-tube has water on one side and oil of density $800\\,\\text{kg/m}^3$ on the other. If water column is $0.3\\,\\text{m}$, find the height of oil column at equilibrium.",
      "image": null,
      "step_by_step": [
        {
          "step": "Equilibrium condition",
          "explanation": "Pressure at bottom equal: $\\rho_w g h_w = \\rho_{oil} g h_{oil}$"
        },
        {
          "step": "Substitute",
          "calculation": "$$ 1000 \\times 9.8 \\times 0.3 = 800 \\times 9.8 \\times h_{oil} $$"
        },
        {
          "step": "Solve",
          "calculation": "$$ h_{oil} = 0.375\\,\\text{m} $$"
        }
      ],
      "final_answer": "Height of oil column is $0.375\\,\\text{m}$."
    },
    {
      "number": 5,
      "problem": "A pipe carries water at $2\\,\\text{m/s}$ in a section of radius $0.1\\,\\text{m}$. Find volumetric flow rate.",
      "image": null,
      "step_by_step": [
        {
          "step": "Flow rate",
          "explanation": "Volumetric flow $Q = A v$, area $A = \\pi r^2$"
        },
        {
          "step": "Substitute",
          "calculation": "$$ Q = \\pi (0.1)^2 \\times 2 \\approx 0.0628\\,\\text{m}^3/\\text{s} $$"
        }
      ],
      "final_answer": "Volumetric flow rate is approximately $0.0628\\,\\text{m}^3/\\text{s}$."
    },
    {
      "number": 6,
      "problem": "Water flows from a tank through a hole at the bottom. Height of water column is $1.2\\,\\text{m}$. Find exit speed using Torricelli’s theorem.",
      "image": null,
      "step_by_step": [
        {
          "step": "Torricelli’s theorem",
          "explanation": "$v = \\sqrt{2 g h}$"
        },
        {
          "step": "Substitute",
          "calculation": "$$ v = \\sqrt{2 \\times 9.8 \\times 1.2} \\approx 4.85\\,\\text{m/s} $$"
        }
      ],
      "final_answer": "Exit speed of water is approximately $4.85\\,\\text{m/s}$."
    },
    {
      "number": 7,
      "problem": "A balloon is submerged in water and experiences an upward buoyant force of $50\\,\\text{N}$. If the weight of the balloon is $30\\,\\text{N}$, find the net force.",
      "image": null,
      "step_by_step": [
        {
          "step": "Net force",
          "explanation": "Net force $F_{net} = F_b - W$"
        },
        {
          "step": "Substitute",
          "calculation": "$$ F_{net} = 50 - 30 = 20\\,\\text{N} $$"
        }
      ],
      "final_answer": "The net upward force is $20\\,\\text{N}$."
    },
    {
      "number": 8,
      "problem": "A liquid flows through a horizontal pipe with areas $A_1 = 0.05\\,\\text{m}^2$ and $A_2 = 0.02\\,\\text{m}^2$. If speed at $A_1$ is $3\\,\\text{m/s}$, find speed at $A_2$ using continuity equation.",
      "image": null,
      "step_by_step": [
        {
          "step": "Continuity equation",
          "explanation": "$A_1 v_1 = A_2 v_2$"
        },
        {
          "step": "Substitute",
          "calculation": "$$ 0.05 \\times 3 = 0.02 \\times v_2 \\implies v_2 = 7.5\\,\\text{m/s} $$"
        }
      ],
      "final_answer": "Speed at $A_2$ is $7.5\\,\\text{m/s}$."
    },
    {
