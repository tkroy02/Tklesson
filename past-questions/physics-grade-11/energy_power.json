{
  "course": "Physics Grade 11",
  "topic": "Work, Energy & Power",
  "problems": [
    {
      "number": 1,
      "problem": "A 50 N force is applied to push a box 4 m across a frictionless surface. Calculate the work done.",
      "image": null,
      "step_by_step": [
        {
          "step": "Work formula",
          "explanation": "Work done $W = F \\cdot d \\cdot \\cos\\theta$, where $\\theta = 0^\\circ$ since force is along displacement."
        },
        {
          "step": "Substitute values",
          "calculation": "$$ W = 50 \\times 4 \\times \\cos 0 = 200\\,\\text{J} $$"
        }
      ],
      "final_answer": "The work done is $200\\,\\text{J}$."
    },
    {
      "number": 2,
      "problem": "A 10 kg mass is lifted vertically 5 m. Find the work done against gravity.",
      "image": null,
      "step_by_step": [
        {
          "step": "Work formula for lifting",
          "explanation": "$W = F \\cdot d$ where $F = mg$"
        },
        {
          "step": "Compute force",
          "calculation": "$$ F = 10 \\times 9.8 = 98\\,\\text{N} $$"
        },
        {
          "step": "Compute work",
          "calculation": "$$ W = 98 \\times 5 = 490\\,\\text{J} $$"
        }
      ],
      "final_answer": "The work done is $490\\,\\text{J}$."
    },
    {
      "number": 3,
      "problem": "A car engine does 5000 J of work in 10 s. Find the power output.",
      "image": null,
      "step_by_step": [
        {
          "step": "Power formula",
          "explanation": "$P = W / t$"
        },
        {
          "step": "Substitute values",
          "calculation": "$$ P = 5000 / 10 = 500\\,\\text{W} $$"
        }
      ],
      "final_answer": "The power output is $500\\,\\text{W}$."
    },
    {
      "number": 4,
      "problem": "A 2 kg object falls 3 m. Find the gravitational potential energy lost.",
      "image": null,
      "step_by_step": [
        {
          "step": "Potential energy formula",
          "explanation": "$\\Delta PE = m g h$"
        },
        {
          "step": "Substitute values",
          "calculation": "$$ \\Delta PE = 2 \\times 9.8 \\times 3 = 58.8\\,\\text{J} $$"
        }
      ],
      "final_answer": "The object loses $58.8\\,\\text{J}$ of potential energy."
    },
    {
      "number": 5,
      "problem": "A 0.5 kg ball moving at 4 m/s. Find its kinetic energy.",
      "image": null,
      "step_by_step": [
        {
          "step": "Kinetic energy formula",
          "explanation": "$KE = \\frac{1}{2} m v^2$"
        },
        {
          "step": "Substitute values",
          "calculation": "$$ KE = 0.5 \\times 0.5 \\times 4^2 = 4\\,\\text{J} $$"
        }
      ],
      "final_answer": "The kinetic energy is $4\\,\\text{J}$."
    },
    {
      "number": 6,
      "problem": "A 100 N force moves a crate 8 m in 4 s. Calculate the power.",
      "image": null,
      "step_by_step": [
        {
          "step": "Compute work done",
          "calculation": "$$ W = F \\cdot d = 100 \\times 8 = 800\\,\\text{J} $$"
        },
        {
          "step": "Compute power",
          "calculation": "$$ P = W / t = 800 / 4 = 200\\,\\text{W} $$"
        }
      ],
      "final_answer": "The power is $200\\,\\text{W}$."
    },
    {
      "number": 7,
      "problem": "A 60 kg skier descends a slope of height 10 m. Find the potential energy at the top.",
      "image": null,
      "step_by_step": [
        {
          "step": "Potential energy formula",
          "explanation": "$PE = m g h$"
        },
        {
          "step": "Substitute values",
          "calculation": "$$ PE = 60 \\times 9.8 \\times 10 = 5880\\,\\text{J} $$"
        }
      ],
      "final_answer": "The potential energy at the top is $5880\\,\\text{J}$."
    },
    {
      "number": 8,
      "problem": "A 2 kg object moving at 3 m/s collides with a wall and stops. Find the work done by the wall.",
      "image": null,
      "step_by_step": [
        {
          "step": "Kinetic energy lost",
          "explanation": "Work done by wall equals change in kinetic energy: $W = \\Delta KE$"
        },
        {
          "step": "Compute kinetic energy",
          "calculation": "$$ KE = 0.5 \\times 2 \\times 3^2 = 9\\,\\text{J} $$"
        }
      ],
      "final_answer": "The wall does $9\\,\\text{J}$ of work on the object."
    },
    {
      "number": 9,
      "problem": "A machine lifts 200 kg to 5 m in 10 s. Calculate power output.",
      "image": null,
      "step_by_step": [
        {
          "step": "Compute work",
          "calculation": "$$ W = m g h = 200 \\times 9.8 \\times 5 = 9800\\,\\text{J} $$"
        },
        {
          "step": "Compute power",
          "calculation": "$$ P = W / t = 9800 / 10 = 980\\,\\text{W} $$"
        }
      ],
      "final_answer": "The power output is $980\\,\\text{W}$."
    },
    {
      "number": 10,
      "problem": "A 0.2 kg ball moving at 10 m/s is caught by a glove. Find the kinetic energy lost.",
      "image": null,
      "step_by_step": [
        {
          "step": "Initial kinetic energy",
          "calculation": "$$ KE = 0.5 \\times 0.2 \\times 10^2 = 10\\,\\text{J} $$"
        },
        {
          "step": "Final kinetic energy",
          "calculation": "$$ KE_{final} = 0\\,\\text{J} $$"
        },
        {
          "step": "Kinetic energy lost",
          "calculation": "$$ \\Delta KE = 10 - 0 = 10\\,\\text{J} $$"
        }
      ],
      "final_answer": "The kinetic energy lost is $10\\,\\text{J}$."
    }
  ]
}
