{
  "course": "Physics Grade 11",
  "topic": "Electricity and Magnetism",
  "problems": [
    {
      "number": 1,
      "problem": "A charge of $q = 2\\,\\mu C$ experiences a force of $F = 0.4\\,N$ in an electric field. Find the field strength.",
      "image": null,
      "step_by_step": [
        {
          "step": "Use electric field formula",
          "calculation": "$$ E = \\frac{F}{q} $$"
        },
        {
          "step": "Substitute values",
          "calculation": "$$ E = \\frac{0.4}{2 \\times 10^{-6}} = 2 \\times 10^5\\,\\text{N/C} $$"
        }
      ],
      "final_answer": "Electric field strength is $2 \\times 10^5\\,\\text{N/C}$."
    },
    {
      "number": 2,
      "problem": "Two point charges, $q_1 = 3\\,\\mu C$ and $q_2 = -3\\,\\mu C$, are 0.5 m apart. Find the force between them.",
      "image": null,
      "step_by_step": [
        {
          "step": "Use Coulomb's law",
          "calculation": "$$ F = k_e \\frac{|q_1 q_2|}{r^2} $$"
        },
        {
          "step": "Substitute values",
          "calculation": "$$ F = 9 \\times 10^9 \\frac{3\\times 10^{-6} \\cdot 3\\times 10^{-6}}{0.5^2} \\approx 0.324\\,N $$"
        }
      ],
      "final_answer": "Force between charges is approximately $0.324\\,N$ attractive."
    },
    {
      "number": 3,
      "problem": "A 12 V battery is connected across a 4 Ω resistor. Find the current.",
      "image": null,
      "step_by_step": [
        {
          "step": "Use Ohm's law",
          "calculation": "$$ I = \\frac{V}{R} $$"
        },
        {
          "step": "Substitute values",
          "calculation": "$$ I = \\frac{12}{4} = 3\\,\\text{A} $$"
        }
      ],
      "final_answer": "Current through the resistor is $3\\,\\text{A}$."
    },
    {
      "number": 4,
      "problem": "A current of 2 A flows through a wire for 3 minutes. Find the total charge transferred.",
      "image": null,
      "step_by_step": [
        {
          "step": "Charge formula",
          "calculation": "$$ Q = I t $$"
        },
        {
          "step": "Convert time to seconds",
          "calculation": "$$ t = 3 \\times 60 = 180\\,s $$"
        },
        {
          "step": "Substitute values",
          "calculation": "$$ Q = 2 \\times 180 = 360\\,C $$"
        }
      ],
      "final_answer": "Total charge transferred is $360\\,C$."
    },
    {
      "number": 5,
      "problem": "A 60 W light bulb is connected to 120 V. Find the current drawn.",
      "image": null,
      "step_by_step": [
        {
          "step": "Use power formula",
          "calculation": "$$ P = VI \\Rightarrow I = \\frac{P}{V} $$"
        },
        {
          "step": "Substitute values",
          "calculation": "$$ I = \\frac{60}{120} = 0.5\\,A $$"
        }
      ],
      "final_answer": "Current drawn is $0.5\\,A$."
    },
    {
      "number": 6,
      "problem": "A coil with 50 turns and area 0.1 m² is in a magnetic field changing at 2 T/s. Find induced EMF.",
      "image": null,
      "step_by_step": [
        {
          "step": "Use Faraday's law",
          "calculation": "$$ \\mathcal{E} = N \\frac{d\\Phi}{dt} $$"
        },
        {
          "step": "Substitute values",
          "calculation": "$$ \\mathcal{E} = 50 \\times 0.1 \\times 2 = 10\\,\\text{V} $$"
        }
      ],
      "final_answer": "Induced EMF is $10\\,V$."
    },
    {
      "number": 7,
      "problem": "A wire carries 5 A current in a magnetic field of 0.2 T. If the wire length is 0.3 m perpendicular to the field, find the force on the wire.",
      "image": null,
      "step_by_step": [
        {
          "step": "Magnetic force formula",
          "calculation": "$$ F = I L B \\sin\\theta $$"
        },
        {
          "step": "Wire perpendicular, so $\\sin\\theta=1$",
          "calculation": "$$ F = 5 \\times 0.3 \\times 0.2 = 0.3\\,N $$"
        }
      ],
      "final_answer": "Force on the wire is $0.3\\,N$."
    },
    {
      "number": 8,
      "problem": "An electron moves at $3\\times10^6$ m/s perpendicular to a 0.1 T magnetic field. Find the radius of its circular path. Use $m_e = 9.11\\times10^{-31}\\,\\text{kg}$ and $e = 1.6\\times10^{-19}\\,C$.",
      "image": null,
      "step_by_step": [
        {
          "step": "Centripetal force equals magnetic force",
          "calculation": "$$ \\frac{mv^2}{r} = qvB \\Rightarrow r = \\frac{mv}{qB} $$"
        },
        {
          "step": "Substitute values",
          "calculation": "$$ r = \\frac{9.11\\times10^{-31} \\times 3\\times10^6}{1.6\\times10^{-19} \\times 0.1} \\approx 0.171\\,\\text{m} $$"
        }
      ],
      "final_answer": "Radius of the circular path is approximately $0.171\\,\\text{m}$."
    },
    {
      "number": 9,
      "problem": "Two parallel wires carry 5 A and 10 A separated by 0.2 m. Find the force per unit length. Use $\\mu_0 = 4\\pi\\times10^{-7}$ T·m/A.",
      "image": null,
      "step_by_step": [
        {
          "step": "Force per unit length formula",
          "calculation": "$$ \\frac{F}{L} = \\frac{\\mu_0 I_1 I_2}{2\\pi d} $$"
        },
        {
          "step": "Substitute values",
          "calculation": "$$ \\frac{F}{L} = \\frac{4\\pi \\times10^{-7} \\cdot 5 \\cdot 10}{2\\pi \\cdot 0.2} = 5\\times10^{-5}\\,\\text{N/m} $$"
        }
      ],
      "final_answer": "Force per unit length is $5\\times10^{-5}\\,\\text{N/m}$ attractive."
    },
    {
      "number": 10,
      "problem": "A solenoid has 200 turns and length 0.5 m. Find the magnetic field inside when current is 2 A. Use $\\mu_0 = 4\\pi\\times10^{-7}$ T·m/A.",
      "image": null,
      "step_by_step": [
        {
          "step": "Magnetic field inside solenoid",
          "calculation": "$$ B = \\mu_0 \\frac{N}{L} I $$"
        },
        {
          "step": "Substitute values",
          "calculation": "$$ B = 4\\pi\\times10^{-7} \\cdot \\frac{200}{0.5} \\cdot 2 \\approx 1.005\\times10^{-3}\\,\\text{T} $$"
        }
      ],
      "final_answer": "Magnetic field inside the solenoid is approximately $1.005\\times10^{-3}\\,\\text{T}$."
    }
  ]
}
