    {
      "number": 1,
      "problem": "A 10 kg box is pushed across a frictionless surface with a force of 50 N. Find its acceleration.",
      "image": null,
      "step_by_step": [
        {
          "step": "Use Newton's Second Law",
          "explanation": "$F = m a$"
        },
        {
          "step": "Solve for $a$",
          "calculation": "$$ a = F / m = 50 / 10 = 5\\,\\text{m/s}^2 $$"
        }
      ],
      "final_answer": "The acceleration of the box is $5\\,\\text{m/s}^2$."
    },
