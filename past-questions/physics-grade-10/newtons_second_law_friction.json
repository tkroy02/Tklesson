    {
      "number": 1,
      "problem": "A 5 kg box is pushed with a net force of $15\\,\\text{N}$. What is its acceleration?",
      "image": null,
      "step_by_step": [
        {
          "step": "Identify given values",
          "explanation": "Mass $m = 5\\,\\text{kg}$, net force $F_{net} = 15\\,\\text{N}$."
        },
        {
          "step": "Apply Newton's Second Law",
          "explanation": "Use $F_{net} = ma$ to find acceleration."
        },
        {
          "step": "Solve for acceleration",
          "explanation": "Rearrange to $a = \\frac{F_{net}}{m}$.",
          "calculation": "$$ a = \\frac{15}{5} = 3\\,\\text{m/s}^2 $$"
        }
      ],
      "final_answer": "The acceleration is $3\\,\\text{m/s}^2$."
    },
    {
      "number": 2,
      "problem": "A 12 kg crate is pulled across the floor with a horizontal force of $40\\,\\text{N}$. The friction force opposing motion is $10\\,\\text{N}$. Find the acceleration.",
      "image": null,
      "step_by_step": [
        {
          "step": "Identify forces",
          "explanation": "Applied force $F_{app} = 40\\,\\text{N}$, friction $F_f = 10\\,\\text{N}$, mass $m = 12\\,\\text{kg}$."
        },
        {
          "step": "Compute net force",
          "explanation": "Net force is $F_{net} = F_{app} - F_f$.",
          "calculation": "$$ F_{net} = 40 - 10 = 30\\,\\text{N} $$"
        },
        {
          "step": "Apply Newton's Second Law",
          "explanation": "Use $a = \\frac{F_{net}}{m}$.",
          "calculation": "$$ a = \\frac{30}{12} = 2.5\\,\\text{m/s}^2 $$"
        }
      ],
      "final_answer": "The acceleration is $2.5\\,\\text{m/s}^2$."
    },
