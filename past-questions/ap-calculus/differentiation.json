  "problems": [
    {
      "number": 1,
      "problem": "Determine $\\dfrac{dy}{dx}$ if $y=\\sin(2x)-3x^2\\tan\\big(5x^2-3x+2\\big)$.",
      "image": null,
      "step_by_step": [
        {
          "step": "Differentiate term-by-term",
          "explanation": "Differentiate each term separately: derivative of $\\sin(2x)$ and derivative of $3x^2\\tan(u)$ with $u=5x^2-3x+2$.",
          "calculation": "y' = d/dx[$\\sin(2x)$] - d/dx[$3x^2\\tan(u)$]."
        },
        {
          "step": "Derivative of $\\sin(2x)$ (chain rule)",
          "explanation": "If $f(x)=\\sin(u)$ with $u=2x$, then $f'(x)=\\cos(u)\\cdot u'$.",
          "calculation": "d/dx[$\\sin(2x)$] = $\\cos(2x)\\cdot 2$\n= $2\\cos(2x)$."
        },
        {
          "step": "Differentiate $3x^2\\tan(u)$ (product rule)",
          "explanation": "Use product rule: if $v=3x^2$ and $w=\\tan(u)$, then d(vw)=v'w + vw'.",
          "calculation": "v = $3x^2$ $\\Rightarrow$ v' = $6x$\nw = $\\tan(u)$ $\\Rightarrow$ w' = $\\sec^2(u)\\cdot u'$."
        },
        {
          "step": "Differentiate inner function $u=5x^2-3x+2$ (chain rule)",
          "explanation": "Compute $u' = d/dx[5x^2-3x+2]$.",
          "calculation": "u' = $10x - 3$."
        },
        {
          "step": "Assemble derivative of the product",
          "explanation": "Combine v', w and v, w' from the product rule and include the chain rule for w'.",
          "calculation": "d/dx[$3x^2\\tan(u)$] = $6x\\tan(u) + 3x^2\\sec^2(u)\\cdot(10x-3)$."
        },
        {
          "step": "Combine results and apply the minus sign",
          "explanation": "Subtract the derivative of the second term from the derivative of the first term.",
          "calculation": "y' = $2\\cos(2x) - \\big(6x\\tan(u) + 3x^2\\sec^2(u)(10x-3)\\big)$."
        }
      ],
      "final_answer": "$\\boxed{\\,y' = 2\\cos(2x) - 6x\\tan\\big(5x^2-3x+2\\big) - 3x^2(10x-3)\\sec^2\\big(5x^2-3x+2\\big)\\,}$"
    },
    {
      "number": 2,
      "problem": "If $2x^2 y + 5x y^2 - \\cos(xy) = 0$, determine $\\dfrac{dy}{dx}$.",
      "image": null,
      "step_by_step": [
        {
          "step": "Differentiate both sides w.r.t. $x$",
          "explanation": "Differentiate each term implicitly; remember $y$ is a function of $x$ so use product rule when terms contain $y$ and include $y'$ where appropriate.",
          "calculation": "d/dx[$2x^2 y$] + d/dx[$5x y^2$] - d/dx[$\\cos(xy)$] = 0."
        },
        {
          "step": "Differentiate $2x^2 y$",
          "explanation": "Use product rule on $x^2 y$: d($x^2 y$)= $2x y + x^2 y'$. Multiply result by 2.",
          "calculation": "d/dx[$2x^2 y$] = $4x y + 2x^2 y'$."
        },
        {
          "step": "Differentiate $5x y^2$",
          "explanation": "Product rule on $x y^2$: d($x y^2$)= $y^2 + x(2y y')$. Multiply by 5.",
          "calculation": "d/dx[$5x y^2$] = $5y^2 + 10x y y'$."
        },
        {
          "step": "Differentiate $-\\cos(xy)$ (chain rule)",
          "explanation": "d/dx[$\\cos(z)$]= $-\\sin(z) z'$; with the outer minus sign becomes $+\\sin(z) z'$. Here $z=xy$.",
          "calculation": "d/dx[$-\\cos(xy)$] = $\\sin(xy) \\cdot d/dx[xy]$\n= $\\sin(xy)(x y' + y)$."
        },
        {
          "step": "Combine derivative terms",
          "explanation": "Collect $y'$ terms on one side and non-$y'$ terms on the other before solving for $y'$.",
          "calculation": "$(4x y + 2x^2 y') + (5y^2 + 10x y y') + \\sin(xy)(x y' + y) = 0$."
        },
        {
          "step": "Group $y'$ terms and solve for $y'$",
          "explanation": "Group coefficients of $y'$ and isolate $y'$ to obtain an explicit expression.",
          "calculation": "$(2x^2 + 10x y + x\\sin(xy))y' + (4x y + 5y^2 + y\\sin(xy)) = 0$\n$\\Rightarrow y' = -\\dfrac{4x y + 5y^2 + y\\sin(xy)}{2x^2 + 10x y + x\\sin(xy)}$."
        }
      ],
      "final_answer": "$\\boxed{\\dfrac{dy}{dx} = -\\dfrac{4xy + 5y^2 + y\\sin(xy)}{2x^2 + 10xy + x\\sin(xy)} }$"
    }
  ]
}
