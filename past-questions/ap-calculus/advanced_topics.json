    {
      "number": 3,
      "problem": "Evaluate the derivative of the parametric equations $x = t^2 + 1$, $y = t^3 - t$ with respect to $t$ at $t = 2$.",
      "image": null,
      "step_by_step": [
        {
          "step": "Compute derivatives",
          "explanation": "Differentiate $x(t)$ and $y(t)$: $\\frac{dx}{dt} = 2t$, $\\frac{dy}{dt} = 3t^2 - 1$"
        },
        {
          "step": "Compute $\\frac{dy}{dx}$",
          "calculation": "$$ \\frac{dy}{dx} = \\frac{dy/dt}{dx/dt} = \\frac{3t^2 - 1}{2t} $$"
        },
        {
          "step": "Substitute $t=2$",
          "calculation": "$$ \\frac{dy}{dx}\\bigg|_{t=2} = \\frac{3(2)^2 - 1}{2(2)} = \\frac{12-1}{4} = \\frac{11}{4} $$"
        }
      ],
      "final_answer": "$\\frac{dy}{dx} = \\frac{11}{4}$ at $t=2$"
    },
    {
      "number": 4,
      "problem": "Solve the differential equation $\\frac{dy}{dx} = 3y$ with initial condition $y(0) = 2$.",
      "image": null,
      "step_by_step": [
        {
          "step": "Separate variables",
          "explanation": "$\\frac{1}{y} dy = 3 dx$"
        },
        {
          "step": "Integrate both sides",
          "calculation": "$$ \\int \\frac{1}{y} dy = \\int 3 dx $$",
          "calculation2": "$$ \\ln|y| = 3x + C $$"
        },
        {
          "step": "Solve for $y$",
          "calculation": "$$ y = e^{3x + C} = Ce^{3x} $$"
        },
        {
          "step": "Apply initial condition",
          "calculation": "$2 = Ce^{3\\cdot0} = C \\implies C = 2$$"
        }
      ],
      "final_answer": "$y = 2 e^{3x}$"
    },
    {
      "number": 5,
      "problem": "Find the sum of the series $\\sum_{n=1}^{\\infty} \\frac{1}{n(n+1)}$.",
      "image": null,
      "step_by_step": [
        {
          "step": "Use partial fraction decomposition",
          "calculation": "$$ \\frac{1}{n(n+1)} = \\frac{1}{n} - \\frac{1}{n+1} $$"
        },
        {
          "step": "Write the telescoping series",
          "calculation": "$$ \\sum_{n=1}^{\\infty} \\left( \\frac{1}{n} - \\frac{1}{n+1} \\right) = 1 - \\frac{1}{2} + \\frac{1}{2} - \\frac{1}{3} + \\dots $$"
        },
        {
          "step": "Compute the limit",
          "explanation": "All intermediate terms cancel",
          "calculation": "$$ S = 1 $$"
        }
      ],
      "final_answer": "$S = 1$"
    },
