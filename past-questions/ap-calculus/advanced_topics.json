      "image": null,
      "step_by_step": [
        {
          "step": "Apply chain rule",
          "calculation": "$$ f'(x) = \\frac{1}{\\sin x} \\cdot \\cos x $$"
        },
        {
          "step": "Simplify",
          "calculation": "$$ f'(x) = \\cot x $$"
