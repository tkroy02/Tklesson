      "step_by_step": [
        {
          "step": "Recall logarithmic integral",
          "explanation": "The integral of $1/x$ is $\\ln|x| + C$."
        }
      ],
      "final_answer": "$\\ln|x| + C$"
    },
    {
      "number": 5,
      "problem": "Evaluate $\\int (3x^2 - 2x + 1) \\, dx$.",
      "image": null,
      "step_by_step": [
        {
          "step": "Integrate term by term",
          "calculation": "$\\int 3x^2 \\, dx = x^3, \\quad \\int -2x \\, dx = -x^2, \\quad \\int 1 \\, dx = x$"
        },
        {
          "step": "Combine results",
          "calculation": "$x^3 - x^2 + x + C$"
        }
      ],
      "final_answer": "$x^3 - x^2 + x + C$"
    },
    {
      "number": 6,
      "problem": "Find $\\int \\sin(4x) \\, dx$.",
      "image": null,
      "step_by_step": [
        {
          "step": "Use substitution or formula",
          "explanation": "Integral of $\\sin(ax)$ is $-\\frac{1}{a} \\cos(ax) + C$."
        },
        {
          "step": "Apply formula",
          "calculation": "$\\int \\sin(4x) \\, dx = -\\frac{1}{4} \\cos(4x) + C$"
        }
      ],
      "final_answer": "$-\\frac{1}{4} \\cos(4x) + C$"
    },
    {
      "number": 7,
      "problem": "Evaluate $\\int \\frac{2x}{x^2 + 1} \\, dx$.",
      "image": null,
      "step_by_step": [
        {
          "step": "Use substitution",
          "explanation": "Let $u = x^2 + 1 \\implies du = 2x dx$"
        },
        {
          "step": "Substitute",
          "calculation": "$\\int \\frac{2x}{x^2 + 1} dx = \\int \\frac{du}{u} = \\ln|u| + C$"
        },
        {
          "step": "Back-substitute",
          "calculation": "$\\ln|x^2+1| + C$"
        }
      ],
      "final_answer": "$\\ln(x^2+1) + C$"
    },
