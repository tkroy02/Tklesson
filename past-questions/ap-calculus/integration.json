    {
      "number": 9,
      "problem": "Evaluate $\\int x e^x \\, dx$.",
      "image": null,
      "step_by_step": [
        {
          "step": "Use integration by parts",
          "explanation": "Formula: $\\int u \\, dv = uv - \\int v \\, du$, let $u=x$, $dv=e^x dx$"
        },
        {
          "step": "Compute derivatives and integrals",
          "calculation": "du = dx, \\quad v = e^x"
        },
        {
          "step": "Apply formula",
          "calculation": "$\\int x e^x dx = x e^x - \\int e^x dx = x e^x - e^x + C = e^x(x-1) + C$"
        }
      ],
      "final_answer": "$e^x(x-1) + C$"
    },
