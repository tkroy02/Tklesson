      "number": 1,
      "problem": "Compute $\\int 5x^3 \\, dx$.",
      "image": null,
      "step_by_step": [
        {
          "step": "Use power rule for integration",
          "explanation": "The integral of $x^n$ is $\\frac{x^{n+1}}{n+1}$, for $n \\neq -1$."
        },
        {
          "step": "Apply rule",
          "calculation": "$\\int 5x^3 \\, dx = 5 \\cdot \\frac{x^{4}}{4} + C = \\frac{5}{4}x^4 + C$"
        }
      ],
      "final_answer": "$\\frac{5}{4}x^4 + C$"
    },
    {
      "number": 2,
      "problem": "Find $\\int \\cos x \\, dx$.",
      "image": null,
      "step_by_step": [
        {
          "step": "Recall standard integral",
          "explanation": "The integral of $\\cos x$ is $\\sin x + C$."
        }
      ],
      "final_answer": "$\\sin x + C$"
    },
    {
      "number": 3,
      "problem": "Evaluate $\\int e^{2x} \\, dx$.",
      "image": null,
      "step_by_step": [
        {
          "step": "Use exponential integral formula",
          "explanation": "The integral of $e^{ax}$ is $\\frac{1}{a} e^{ax} + C$."
        },
        {
          "step": "Apply formula",
          "calculation": "$\\int e^{2x} \\, dx = \\frac{1}{2} e^{2x} + C$"
        }
      ],
      "final_answer": "$\\frac{1}{2} e^{2x} + C$"
    },
    {
      "number": 4,
      "problem": "Compute $\\int \\frac{1}{x} \\, dx$.",
