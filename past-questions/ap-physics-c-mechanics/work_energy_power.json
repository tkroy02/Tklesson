  "topic": "Work, Energy, and Power",
  "problems": [
    {
      "number": 1,
      "problem": "A 5 kg block is pushed horizontally with a constant force of 20 N over 4 m on a frictionless surface. Find the work done by the force.",
      "image": null,
      "step_by_step": [
        {
          "step": "Identify variables",
          "explanation": "Force $F = 20 \\, \\text{N}$, displacement $d = 4 \\, \\text{m}$, angle $\\theta = 0^\\circ$"
        },
        {
          "step": "Work formula",
          "explanation": "$W = F d \\cos \\theta$"
        },
        {
          "step": "Substitute values",
          "calculation": "$W = 20 * 4 * \\cos 0^\\circ = 80 \\, \\text{J}$"
        }
      ],
      "final_answer": "Work done: $W = 80 \\, \\text{J}$"
    },
    {
      "number": 2,
      "problem": "A 2 kg mass falls freely from a height of 5 m. Find the gravitational potential energy lost.",
      "image": null,
      "step_by_step": [
        {
          "step": "Potential energy formula",
          "explanation": "$PE = m g h$"
        },
        {
          "step": "Substitute values",
          "calculation": "$PE = 2 * 9.8 * 5 = 98 \\, \\text{J}$"
        }
      ],
      "final_answer": "Gravitational potential energy lost: $98 \\, \\text{J}$"
    },
    {
      "number": 3,
      "problem": "A 3 kg object is moving at 4 m/s. Calculate its kinetic energy.",
      "image": null,
      "step_by_step": [
        {
          "step": "Kinetic energy formula",
          "explanation": "$KE = \\frac{1}{2} m v^2$"
        },
        {
          "step": "Substitute values",
          "calculation": "$KE = 0.5 * 3 * 4^2 = 24 \\, \\text{J}$"
        }
      ],
      "final_answer": "Kinetic energy: $KE = 24 \\, \\text{J}$"
    },
    {
      "number": 4,
      "problem": "A 4 kg block slides down a frictionless incline of height 3 m. Find its speed at the bottom.",
      "image": null,
      "step_by_step": [
        {
          "step": "Energy conservation",
          "explanation": "Potential energy at top converts to kinetic energy at bottom: $m g h = 0.5 m v^2$"
        },
        {
          "step": "Solve for $v$",
          "explanation": "$v = \\sqrt{2 g h} = \\sqrt{2*9.8*3} \\approx 7.67 \\, \\text{m/s}$"
        }
      ],
      "final_answer": "Speed at bottom: $v \\approx 7.67 \\, \\text{m/s}$"
    },
    {
      "number": 5,
      "problem": "A force of 50 N acts on a 10 kg box moving at 2 m/s. Calculate the power developed by the force if the box moves 6 m in 3 s.",
      "image": null,
      "step_by_step": [
        {
          "step": "Work done",
          "explanation": "$W = F d = 50 * 6 = 300 \\, \\text{J}$"
        },
        {
          "step": "Power formula",
          "explanation": "$P = W / t = 300 / 3 = 100 \\, \\text{W}$"
        }
      ],
      "final_answer": "Power developed: $P = 100 \\, \\text{W}$"
    },
    {
      "number": 6,
      "problem": "A 2 kg mass is lifted vertically at constant speed to a height of 8 m. Find the work done against gravity.",
      "image": null,
      "step_by_step": [
        {
          "step": "Work against gravity",
          "explanation": "$W = m g h = 2 * 9.8 * 8 = 156.8 \\, \\text{J}$"
        }
      ],
      "final_answer": "Work done: $W = 156.8 \\, \\text{J}$"
    },
    {
      "number": 7,
      "problem": "A 5 kg mass moving at 3 m/s collides with a spring and compresses it by 0.2 m. Find the spring constant $k$ if all kinetic energy is stored in the spring.",
      "image": null,
      "step_by_step": [
        {
          "step": "Kinetic energy",
          "explanation": "$KE = 0.5 m v^2 = 0.5 * 5 * 3^2 = 22.5 \\, \\text{J}$"
        },
        {
          "step": "Spring potential energy",
          "explanation": "$PE_s = 0.5 k x^2$, set equal to $KE$: $22.5 = 0.5 k * 0.2^2$"
        },
        {
          "step": "Solve for $k$",
          "calculation": "$k = 22.5 / (0.5*0.04) = 22.5 / 0.02 = 1125 \\, \\text{N/m}$"
        }
      ],
      "final_answer": "Spring constant: $k = 1125 \\, \\text{N/m}$"
    },
    {
      "number": 8,
      "problem": "A 1000 kg car accelerates from 0 to 20 m/s in 10 s. Find the average power produced.",
      "image": null,
      "step_by_step": [
        {
          "step": "Kinetic energy",
          "explanation": "$KE = 0.5 m v^2 = 0.5 * 1000 * 20^2 = 200,000 \\, \\text{J}$"
        },
        {
          "step": "Power",
          "explanation": "$P = KE / t = 200,000 / 10 = 20,000 \\, \\text{W}$"
        }
      ],
      "final_answer": "Average power: $P = 20,000 \\, \\text{W}$"
    },
    {
      "number": 9,
      "problem": "A 3 kg mass slides down a 4 m high hill. How much speed does it have at the bottom?",
      "image": null,
      "step_by_step": [
        {
          "step": "Energy conservation",
          "explanation": "$m g h = 0.5 m v^2 \\implies v = \\sqrt{2 g h} = \\sqrt{2*9.8*4} \\approx 8.85 \\, \\text{m/s}$"
        }
      ],
      "final_answer": "Speed at bottom: $v \\approx 8.85 \\, \\text{m/s}$"
    },
    {
      "number": 10,
      "problem": "A 2 kg object moves horizontally at 5 m/s. A force of 10 N acts over 3 s. Find the work done by the force and the final kinetic energy.",
      "image": null,
      "step_by_step": [
        {
          "step": "Acceleration",
          "explanation": "$a = F / m = 10 / 2 = 5 \\, \\text{m/s}^2$"
        },
        {
          "step": "Final velocity",
          "explanation": "$v_f = v_0 + a t = 5 + 5*3 = 20 \\, \\text{m/s}$"
        },
        {
          "step": "Kinetic energy",
          "explanation": "$KE = 0.5 m v_f^2 = 0.5*2*20^2 = 400 \\, \\text{J}$"
        },
        {
          "step": "Work done",
          "explanation": "Work equals change in kinetic energy: $W = \\Delta KE = 400 - 0.5*2*5^2 = 400 - 25 = 375 \\, \\text{J}$"
        }
      ],
      "final_answer": "Work done: $W = 375 \\, \\text{J}$, Final KE: $KE = 400 \\, \\text{J}$"
    }
  ]
}
