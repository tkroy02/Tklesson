    {
      "number": 2,
      "problem": "A car moves with velocity $20 \\, \\text{m/s}$ and accelerates at $2 \\, \\text{m/s}^2$ for $5$ seconds. Find the displacement.",
      "image": null,
      "step_by_step": [
        {
          "step": "Use displacement equation",
          "explanation": "$x = v_0 t + \\frac{1}{2} a t^2$"
        },
        {
          "step": "Substitute values",
          "calculation": "$x = 20*5 + 0.5*2*5^2 = 100 + 25 = 125 \\, \\text{m}$"
        }
      ],
      "final_answer": "Displacement: $x = 125 \\, \\text{m}$"
    },
    {
      "number": 3,
      "problem": "A projectile is launched vertically with $v_0 = 30 \\, \\text{m/s}$. Calculate maximum height. Use $g = 10 \\, \\text{m/s}^2$.",
      "image": null,
      "step_by_step": [
        {
          "step": "Use kinematic equation",
          "explanation": "$v^2 = v_0^2 - 2 g h_{max}$"
        },
        {
          "step": "Rearrange for height",
          "explanation": "$h_{max} = \\frac{v_0^2}{2g}$"
        },
        {
          "step": "Substitute values",
          "calculation": "$h_{max} = \\frac{30^2}{2*10} = \\frac{900}{20} = 45 \\, \\text{m}$"
        }
      ],
      "final_answer": "Maximum height: $h_{max} = 45 \\, \\text{m}$"
    },
    {
      "number": 4,
      "problem": "A particle has position $x(t) = 5 t^2 - 2 t$. Find its velocity and acceleration at $t = 3 \\, \\text{s}$.",
      "image": null,
      "step_by_step": [
        {
