      "step_by_step": [
        {
          "step": "Identify given values",
          "explanation": "Initial velocity $v_0 = 0$, acceleration $a = 4 \\, \\text{m/s}^2$, time $t = 3 \\, \\text{s}$"
        },
        {
          "step": "Use kinematic equation",
          "explanation": "$v = v_0 + a t$"
        },
        {
          "step": "Substitute values",
          "calculation": "$v = 0 + 4*3 = 12 \\, \\text{m/s}$"
        }
      ],
      "final_answer": "Velocity: $v = 12 \\, \\text{m/s}$"
