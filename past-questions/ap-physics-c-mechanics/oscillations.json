{
  "course": "AP Physics C: Mechanics",
  "topic": "Oscillations",
  "problems": [
    {
      "number": 1,
      "problem": "A mass of 0.5 kg is attached to a spring with spring constant $k = 200 \\, \\text{N/m}$. Find the period of oscillation.",
      "image": null,
      "step_by_step": [
        {
          "step": "Use period formula",
          "explanation": "For mass-spring system: $T = 2\\pi \\sqrt{m/k}$"
        },
        {
          "step": "Substitute values",
          "calculation": "$T = 2\\pi \\sqrt{0.5/200} = 2\\pi \\sqrt{0.0025} = 2\\pi * 0.05 \\approx 0.314 \\, \\text{s}$"
        }
      ],
      "final_answer": "Period: $T \\approx 0.314 \\, \\text{s}$"
    },
    {
      "number": 2,
      "problem": "A pendulum of length 1.2 m swings with small amplitude. Find its period.",
      "image": null,
      "step_by_step": [
        {
          "step": "Pendulum period formula",
          "explanation": "$T = 2\\pi \\sqrt{L/g}$"
        },
        {
          "step": "Substitute values",
          "calculation": "$T = 2\\pi \\sqrt{1.2/9.8} = 2\\pi \\sqrt{0.1224} \\approx 2\\pi * 0.35 \\approx 2.20 \\, \\text{s}$"
        }
      ],
      "final_answer": "Period: $T \\approx 2.20 \\, \\text{s}$"
    },
    {
      "number": 3,
      "problem": "A mass oscillates with amplitude 0.1 m and angular frequency $\\omega = 5 \\, \\text{rad/s}$. Find the maximum speed.",
      "image": null,
      "step_by_step": [
        {
          "step": "Maximum speed formula",
          "explanation": "$v_{max} = \\omega A$"
        },
        {
          "step": "Substitute values",
          "calculation": "$v_{max} = 5*0.1 = 0.5 \\, \\text{m/s}$"
        }
      ],
      "final_answer": "Maximum speed: $v_{max} = 0.5 \\, \\text{m/s}$"
    },
    {
      "number": 4,
      "problem": "A mass-spring system oscillates with amplitude 0.2 m and period 1 s. Find the maximum acceleration.",
      "image": null,
      "step_by_step": [
        {
          "step": "Angular frequency",
          "explanation": "$\\omega = 2\\pi/T = 2\\pi/1 = 2\\pi \\, \\text{rad/s}$"
        },
        {
          "step": "Maximum acceleration",
          "explanation": "$a_{max} = \\omega^2 A = (2\\pi)^2 * 0.2 \\approx 39.48 * 0.2 \\approx 7.90 \\, \\text{m/s}^2$"
        }
      ],
      "final_answer": "Maximum acceleration: $a_{max} \\approx 7.90 \\, \\text{m/s}^2$"
    },
    {
      "number": 5,
      "problem": "A simple pendulum has period 2 s. What is its frequency?",
      "image": null,
      "step_by_step": [
        {
          "step": "Frequency formula",
          "explanation": "$f = 1/T$"
        },
        {
          "step": "Substitute",
          "calculation": "$f = 1/2 = 0.5 \\, \\text{Hz}$"
        }
      ],
      "final_answer": "Frequency: $f = 0.5 \\, \\text{Hz}$"
    },
    {
      "number": 6,
      "problem": "A 0.25 kg mass oscillates on a spring with $k = 100 \\, \\text{N/m}$. Find the angular frequency.",
      "image": null,
      "step_by_step": [
        {
          "step": "Angular frequency formula",
          "explanation": "$\\omega = \\sqrt{k/m}$"
        },
        {
          "step": "Substitute",
          "calculation": "$\\omega = \\sqrt{100/0.25} = \\sqrt{400} = 20 \\, \\text{rad/s}$"
        }
      ],
      "final_answer": "Angular frequency: $\\omega = 20 \\, \\text{rad/s}$"
    },
    {
      "number": 7,
      "problem": "A pendulum is displaced 0.05 m from equilibrium. Find its maximum acceleration if $g = 9.8 \\, \\text{m/s}^2$ and $L = 1 m$.",
      "image": null,
      "step_by_step": [
        {
          "step": "Angular frequency",
          "explanation": "$\\omega = \\sqrt{g/L} = \\sqrt{9.8/1} \\approx 3.13 \\, \\text{rad/s}$"
        },
        {
          "step": "Maximum acceleration",
          "explanation": "$a_{max} = \\omega^2 A = 3.13^2 * 0.05 \\approx 9.8*0.05 \\approx 0.49 \\, \\text{m/s}^2$"
        }
      ],
      "final_answer": "Maximum acceleration: $a_{max} \\approx 0.49 \\, \\text{m/s}^2$"
    },
    {
      "number": 8,
      "problem": "A 0.3 kg mass-spring system has total mechanical energy 0.6 J. Find the amplitude if $k = 80 \\, \\text{N/m}$.",
      "image": null,
      "step_by_step": [
        {
          "step": "Energy in SHM",
          "explanation": "Total energy $E = 1/2 k A^2$"
        },
        {
          "step": "Solve for $A$",
          "calculation": "$A = \\sqrt{2E/k} = \\sqrt{2*0.6/80} = \\sqrt{1.2/80} = \\sqrt{0.015} \\approx 0.122 \\, \\text{m}$"
        }
      ],
      "final_answer": "Amplitude: $A \\approx 0.122 \\, \\text{m}$"
    },
    {
      "number": 9,
      "problem": "A pendulum swings with maximum speed 1 m/s. Find its amplitude if $L = 1 m$ and $g = 9.8 \\, \\text{m/s}^2$.",
      "image": null,
      "step_by_step": [
        {
          "step": "Maximum speed for pendulum",
          "explanation": "$v_{max} = \\omega A$, $\\omega = \\sqrt{g/L} = \\sqrt{9.8/1} \\approx 3.13$"
        },
        {
          "step": "Solve for $A$",
          "calculation": "$A = v_{max}/\\omega = 1/3.13 \\approx 0.319 \\, \\text{m}$"
        }
      ],
      "final_answer": "Amplitude: $A \\approx 0.319 \\, \\text{m}$"
    },
    {
      "number": 10,
      "problem": "A mass-spring system oscillates with $A = 0.1 m$, $\\omega = 6 rad/s$. Find the displacement at $t = 0.05 s$ if initial phase $\\phi = 0$.",
      "image": null,
      "step_by_step": [
        {
          "step": "Displacement formula",
          "explanation": "$x(t) = A \\cos(\\omega t + \\phi)$"
        },
        {
          "step": "Substitute values",
          "calculation": "$x(0.05) = 0.1 \\cos(6*0.05 + 0) = 0.1 \\cos(0.3) \\approx 0.1*0.955 = 0.0955 \\, \\text{m}$"
        }
      ],
      "final_answer": "Displacement at $t=0.05 s$: $x \\approx 0.0955 \\, \\text{m}$"
    }
  ]
}
