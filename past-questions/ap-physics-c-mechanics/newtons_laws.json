{
  "course": "AP Physics C: Mechanics",
  "topic": "Newton's Laws",
  "problems": [
    {
      "number": 1,
      "problem": "A 5 kg block rests on a horizontal surface. If a horizontal force of 10 N is applied, what is the acceleration? Assume no friction.",
      "image": null,
      "step_by_step": [
        {
          "step": "Identify forces",
          "explanation": "Net force $F_{net} = 10 \\, \\text{N}$, mass $m = 5 \\, \\text{kg}$"
        },
        {
          "step": "Apply Newton's Second Law",
          "explanation": "$F_{net} = m a \\implies a = \\frac{F_{net}}{m}$"
        },
        {
          "step": "Substitute values",
          "calculation": "$a = 10 / 5 = 2 \\, \\text{m/s}^2$"
        }
      ],
      "final_answer": "Acceleration: $a = 2 \\, \\text{m/s}^2$"
    },
    {
      "number": 2,
      "problem": "A 10 kg block slides on a surface with friction coefficient $\\mu = 0.2$. What is the acceleration when a 30 N horizontal force is applied?",
      "image": null,
      "step_by_step": [
        {
          "step": "Friction force",
          "explanation": "Friction $f = \\mu m g = 0.2 * 10 * 9.8 = 19.6 \\, \\text{N}$"
        },
        {
          "step": "Net force",
          "explanation": "$F_{net} = 30 - 19.6 = 10.4 \\, \\text{N}$"
        },
        {
          "step": "Acceleration",
          "explanation": "$a = F_{net}/m = 10.4 / 10 = 1.04 \\, \\text{m/s}^2$"
        }
      ],
      "final_answer": "Acceleration: $a = 1.04 \\, \\text{m/s}^2$"
    },
    {
      "number": 3,
      "problem": "A 2 kg object is in free fall. Find its acceleration.",
      "image": null,
      "step_by_step": [
        {
          "step": "Identify forces",
          "explanation": "Only gravity acts: $F_g = m g = 2 * 9.8 = 19.6 \\, \\text{N}$"
        },
        {
          "step": "Newton's Second Law",
          "explanation": "$a = F_{net}/m = 19.6 / 2 = 9.8 \\, \\text{m/s}^2$"
        }
      ],
      "final_answer": "Acceleration: $a = 9.8 \\, \\text{m/s}^2$"
    },
    {
      "number": 4,
      "problem": "A 5 kg block is pulled with a force of 25 N at an angle $30^\\circ$ above the horizontal on a frictionless surface. Find the acceleration.",
      "image": null,
      "step_by_step": [
        {
          "step": "Horizontal force component",
          "explanation": "$F_x = F \\cos \\theta = 25 * \\cos 30^\\circ \\approx 25 * 0.866 = 21.65 \\, \\text{N}$"
        },
        {
          "step": "Newton's Second Law",
          "explanation": "$a = F_x / m = 21.65 / 5 \\approx 4.33 \\, \\text{m/s}^2$"
        }
      ],
      "final_answer": "Acceleration: $a \\approx 4.33 \\, \\text{m/s}^2$"
    },
    {
      "number": 5,
      "problem": "Two blocks, $m_1 = 3$ kg and $m_2 = 5$ kg, are connected over a frictionless pulley. Find acceleration of the system.",
      "image": null,
      "step_by_step": [
        {
          "step": "Net force",
          "explanation": "Gravity acts on $m_2$: $F_{net} = m_2 g - m_1 g = (5-3)*9.8 = 19.6 \\, \\text{N}$"
        },
        {
          "step": "Total mass",
          "explanation": "$m_{total} = m_1 + m_2 = 8 \\, \\text{kg}$"
        },
        {
          "step": "Acceleration",
          "explanation": "$a = F_{net}/m_{total} = 19.6 / 8 = 2.45 \\, \\text{m/s}^2$"
        }
      ],
      "final_answer": "Acceleration: $a = 2.45 \\, \\text{m/s}^2$"
    },
    {
      "number": 6,
      "problem": "A 4 kg box rests on a rough horizontal surface. The coefficient of friction is 0.3. Determine the minimum force needed to start motion.",
      "image": null,
      "step_by_step": [
        {
          "step": "Static friction",
          "explanation": "$f_s = \\mu_s m g = 0.3 * 4 * 9.8 = 11.76 \\, \\text{N}$"
        },
        {
          "step": "Minimum applied force",
          "explanation": "Force must exceed static friction: $F_{min} = 11.76 \\, \\text{N}$"
        }
      ],
      "final_answer": "Minimum force: $F_{min} = 11.76 \\, \\text{N}$"
    },
    {
      "number": 7,
      "problem": "A 10 kg box is pulled by a rope with tension $50$ N at $20^\\circ$ above horizontal. Friction coefficient $0.2$. Find acceleration.",
      "image": null,
      "step_by_step": [
        {
          "step": "Horizontal component",
          "explanation": "$F_x = T \\cos 20^\\circ = 50 * 0.940 = 47 \\, \\text{N}$"
        },
        {
          "step": "Friction force",
          "explanation": "$f = \\mu m g = 0.2*10*9.8 = 19.6 \\, \\text{N}$"
        },
        {
          "step": "Net force",
          "explanation": "$F_{net} = 47 - 19.6 = 27.4 \\, \\text{N}$"
        },
        {
          "step": "Acceleration",
          "explanation": "$a = F_{net}/m = 27.4 / 10 = 2.74 \\, \\text{m/s}^2$"
        }
      ],
      "final_answer": "Acceleration: $a = 2.74 \\, \\text{m/s}^2$"
    },
    {
      "number": 8,
      "problem": "A 3 kg object is suspended by a rope. What is the tension if it accelerates upward at $2 \\, \\text{m/s}^2$?",
      "image": null,
      "step_by_step": [
        {
          "step": "Forces",
          "explanation": "Upward tension $T$, downward weight $W = m g$"
        },
        {
          "step": "Newton's Second Law",
          "explanation": "$T - W = m a \\implies T = m g + m a = 3*9.8 + 3*2 = 29.4 + 6 = 35.4 \\, \\text{N}$"
        }
      ],
      "final_answer": "Tension: $T = 35.4 \\, \\text{N}$"
    },
    {
      "number": 9,
      "problem": "A 6 kg object rests on a frictionless incline $30^\\circ$. Find acceleration down the incline.",
      "image": null,
      "step_by_step": [
        {
          "step": "Force along incline",
          "explanation": "$F = m g \\sin \\theta = 6*9.8*0.5 = 29.4 \\, \\text{N}$"
        },
        {
          "step": "Acceleration",
          "explanation": "$a = F/m = 29.4 / 6 = 4.9 \\, \\text{m/s}^2$"
        }
      ],
      "final_answer": "Acceleration: $a = 4.9 \\, \\text{m/s}^2$"
    },
    {
      "number": 10,
      "problem": "A 8 kg box is at rest. A horizontal force of 40 N is applied. Friction is 5 N. Find acceleration.",
      "image": null,
      "step_by_step": [
        {
          "step": "Net force",
          "explanation": "$F_{net} = 40 - 5 = 35 \\, \\text{N}$"
        },
        {
          "step": "Acceleration",
          "explanation": "$a = F_{net}/m = 35/8 \\approx 4.375 \\, \\text{m/s}^2$"
        }
      ],
      "final_answer": "Acceleration: $a \\approx 4.38 \\, \\text{m/s}^2$"
    }
  ]
