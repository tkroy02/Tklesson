    {
      "number": 9,
      "problem": "A 2 kg block is moving at 3 m/s and collides elastically with another 2 kg block at rest. Find speed of first block after collision (1D elastic).",
      "image": null,
      "step_by_step": [
        {
          "step": "Elastic collision formula",
          "explanation": "$v_1' = v_1 (m_1 - m_2)/(m_1 + m_2) + v_2 (2 m_2)/(m_1 + m_2)$"
        },
        {
          "step": "Substitute",
          "calculation": "$v_1' = 3*(2-2)/(2+2) + 0*(4/4) = 0$"
        }
      ],
      "final_answer": "First block comes to rest: $v_1' = 0 \\, \\text{m/s}$"
    },
    {
      "number": 10,
      "problem": "A 500 W motor lifts a 50 kg mass vertically at 2 m/s. Find force exerted and distance lifted in 10 s.",
      "image": null,
      "step_by_step": [
        {
          "step": "Power formula",
          "explanation": "$P = F v \\Rightarrow F = P/v$"
        },
        {
          "step": "Calculate force",
          "calculation": "$F = 500/2 = 250 \\, \\text{N}$"
        },
        {
          "step": "Distance in 10 s",
          "explanation": "$d = v t = 2*10 = 20 \\, \\text{m}$"
        }
      ],
      "final_answer": "Force: $F = 250 \\, \\text{N}$, Distance lifted: $d = 20 \\, \\text{m}$"
    }
