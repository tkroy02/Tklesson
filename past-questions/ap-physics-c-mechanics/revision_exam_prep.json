  "problems": [
    {
      "number": 1,
      "problem": "A 5 kg block slides down a frictionless incline of height 4 m. Find its speed at the bottom.",
      "image": null,
      "step_by_step": [
        {
          "step": "Use energy conservation",
          "explanation": "Potential energy at top converts to kinetic energy at bottom: $mgh = \\frac{1}{2}mv^2$"
        },
        {
          "step": "Solve for $v$",
          "calculation": "$v = \\sqrt{2gh} = \\sqrt{2*9.8*4} \\approx 8.85 \\, \\text{m/s}$"
        }
      ],
      "final_answer": "Speed at bottom: $v \\approx 8.85 \\, \\text{m/s}$"
    },
    {
      "number": 2,
      "problem": "A satellite orbits Earth at $7.0 \\times 10^6$ m from the center. Find its orbital speed.",
      "image": null,
      "step_by_step": [
        {
          "step": "Use $v = \\sqrt{GM/r}$",
          "explanation": "Gravitational constant $G$ and Earth's mass $M$ known."
        },
        {
          "step": "Substitute",
          "calculation": "$v = \\sqrt{6.674\\times10^{-11} * 5.972\\times10^{24} / 7.0\\times10^6} \\approx 7.55\\times10^3 \\, \\text{m/s}$"
        }
      ],
      "final_answer": "Orbital speed: $v \\approx 7.55\\text{ km/s}$"
    },
    {
      "number": 3,
      "problem": "A car accelerates uniformly from 0 to 25 m/s in 10 s. Find acceleration and distance covered.",
      "image": null,
      "step_by_step": [
        {
          "step": "Acceleration formula",
          "explanation": "$a = (v - v_0)/t$"
        },
        {
          "step": "Calculate $a$",
          "calculation": "$a = (25-0)/10 = 2.5 \\, \\text{m/s}^2$"
        },
        {
          "step": "Distance formula",
          "explanation": "$s = v_0 t + 0.5 a t^2$"
        },
        {
          "step": "Calculate $s$",
          "calculation": "$s = 0 + 0.5 * 2.5 * 10^2 = 125 \\, \\text{m}$"
        }
      ],
      "final_answer": "Acceleration: $2.5 \\, \\text{m/s}^2$, Distance: $125 \\, \\text{m}$"
    },
    {
      "number": 4,
      "problem": "A 2 kg object is subjected to 10 N force. Find acceleration.",
      "image": null,
      "step_by_step": [
        {
          "step": "Newton's second law",
          "explanation": "$F = ma \\Rightarrow a = F/m$"
        },
        {
          "step": "Substitute",
          "calculation": "$a = 10/2 = 5 \\, \\text{m/s}^2$"
        }
      ],
      "final_answer": "Acceleration: $a = 5 \\, \\text{m/s}^2$"
    },
    {
      "number": 5,
      "problem": "Two masses, 3 kg and 5 kg, connected by a string on a frictionless surface. Find acceleration if 16 N is applied to 5 kg mass.",
      "image": null,
      "step_by_step": [
        {
          "step": "Total mass",
          "explanation": "M_total = 3+5 = 8 kg"
        },
        {
          "step": "Acceleration",
          "calculation": "$a = F/M_total = 16/8 = 2 \\, \\text{m/s}^2$"
        }
      ],
      "final_answer": "Acceleration: $a = 2 \\, \\text{m/s}^2$"
    },
    {
      "number": 6,
      "problem": "A pendulum of length 2 m swings from 30° with max speed at bottom. Find max speed.",
      "image": null,
      "step_by_step": [
        {
          "step": "Use energy conservation",
          "explanation": "$mgh = 0.5 m v^2$, $h = L(1 - \\cos\\theta)$"
        },
        {
          "step": "Calculate $h$",
          "calculation": "$h = 2*(1 - \\cos30°) \\approx 0.268 m$"
        },
        {
          "step": "Solve for $v$",
          "calculation": "$v = \\sqrt{2*9.8*0.268} \\approx 2.29 \\, \\text{m/s}$"
        }
      ],
      "final_answer": "Maximum speed: $v \\approx 2.29 \\, \\text{m/s}$"
    },
    {
