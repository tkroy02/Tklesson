    {
      "number": 3,
      "problem": "A 5 kg object is subject to two forces: $10 \\, \\text{N}$ to the right and $4 \\, \\text{N}$ to the left. Find the net force and acceleration.",
      "image": null,
      "step_by_step": [
        {
          "step": "Identify forces",
          "explanation": "$F_1 = 10 \\, \\text{N}$, $F_2 = 4 \\, \\text{N}$ (opposite direction)"
        },
        {
          "step": "Net force",
          "calculation": "$F_{net} = F_1 - F_2 = 10 - 4 = 6 \\, \\text{N}$"
        },
        {
          "step": "Acceleration",
          "explanation": "$a = \\frac{F_{net}}{m}$",
          "calculation": "$a = \\frac{6}{5} = 1.2 \\, \\text{m/s}^2$"
        }
      ],
      "final_answer": "$F_{net} = 6 \\, \\text{N}$, $a = 1.2 \\, \\text{m/s}^2$"
    },
    {
