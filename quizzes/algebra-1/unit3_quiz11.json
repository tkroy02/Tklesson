      {
        "type": "mcq",
        "question": "If $f(x)=x+5$, $g(x)=2x-3$, and $h(x)=4-x$, find $(f+g+h)(x)$.",
        "options": [
          "$2x+6$",
          "$4x+6$",
          "$3x+6$",
          "$2x+12$"
        ],
        "answer": "A",
        "explanation": "$(f+g+h)(x) = (x+5) + (2x-3) + (4-x) = 2x + 6$."
      },
      {
        "type": "mcq",
        "question": "Given $f(x)=x+5$, $g(x)=2x-3$, and $h(x)=4-x$, find $(f-g-h)(x)$.",
        "options": [
          "$2x+4$",
          "$-2x+6$",
          "$4$",
          "$0$"
        ],
        "answer": "C",
        "explanation": "$(f-g-h)(x) = (x+5) - (2x-3) - (4-x) = x+5 - 2x + 3 - 4 + x = 4$."
      },
      {
        "type": "mcq",
        "question": "Find $(2f+g)(x)$ using $f(x)=x+5$ and $g(x)=2x-3$.",
        "options": [
          "$3x+7$",
          "$4x-7$",
          "$4x+7$",
          "$4x+2$"
        ],
        "answer": "C",
        "explanation": "$(2f+g)(x) = 2(x+5) + (2x-3) = 2x+10 + 2x-3 = 4x+7$."
      },
      {
        "type": "mcq",
        "question": "Find $(3g-h)(x)$ using $g(x)=2x-3$ and $h(x)=4-x$.",
        "options": [
          "$7x-5$",
          "$6x-13$",
          "$5x-13$",
          "$7x-13$"
        ],
        "answer": "D",
        "explanation": "$(3g-h)(x) = 3(2x-3) - (4-x) = 6x-9 - 4 + x = 7x-13$."
      },
      {
        "type": "mcq",
        "question": "Using $f(x)=x+5$, $g(x)=2x-3$, and $h(x)=4-x$, find $(f+2g-3h)(x)$.",
        "options": [
          "$2x-13$",
          "$8x-13$",
          "$8x+11$",
          "$-13$"
        ],
        "answer": "B",
        "explanation": "$(f+2g-3h)(x) = (x+5) + 2(2x-3) - 3(4-x) = 8x - 13$."
      },
      {
        "type": "mcq",
        "question": "Calculate $(f \\cdot g + h)(x)$ given $f(x)=x+5$, $g(x)=2x-3$, and $h(x)=4-x$.",
